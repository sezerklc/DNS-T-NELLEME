    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:DNS_TÜNELLEME"
    Shell.FlyoutBehavior="Flyout"
    Title="DNS_TÜNELLEME">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
    <ShellContent 
        Title="Hakkımızda"
        ContentTemplate="{DataTemplate local:Hakkımızda}"
        Route="Hakkımızda"/>
    <ShellContent 
        Title="Ana Sayfa"
        ContentTemplate="{DataTemplate local:HomePage}"
        Route="HomePage"/>
</Shell>
    
